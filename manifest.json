{
  "manifest_version": 3,
  "name": "Dynafresh",
  "version": "0.1",
  "description": "Custom autorefresh for Dynatrace",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "scripting", "webNavigation", "nativeMessaging"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["rules_collection.json"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ]
}
